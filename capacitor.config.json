{
  "appId": "com.ipro.app",
  "appName": "iPro",
  "webDir": "www",
  "bundledWebRuntime": false,
  "plugins": {
    "SplashScreen": {
      "backgroundColor": "#041934",
      "launchShowDuration": 3000,
      "launchAutoHide": false,
      "androidScaleType": "CENTER_CROP",
      "showSpinner": false
    },
    "PushNotifications": {
      "presentationOptions": [
        "badge",
        "sound",
        "alert"
      ]
    },
    "server": {
      "allowNavigation": [
        "*"
      ]
    }
  }
}
